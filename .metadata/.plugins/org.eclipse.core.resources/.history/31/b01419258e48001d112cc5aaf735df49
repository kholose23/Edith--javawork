package Methodref;

public class school {
	public void noteSomething(String x)
	{
		System.out.println("Make a note of "+x);
	}

	public void noteObservation(String x);
	{
		System.out.println("write "+x+" in your notebook");
	}

	public void noteOnlineClass(String x)
	{
		System.out.println("write "+x+" in your notebook");
	}
	
}

