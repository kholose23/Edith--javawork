package Employee;

import java.io.serialisation

public class employee {

	public class Product implements serialisable {
		public int Id;
	    public String email;
	    public transient int salary;
	    public transient int designation;
	    public String name;
	    public employee(intId, String name, int salary, String email, int designation) {
	    	super();
	    	this.Id = Id;
	    	this.name = name;
	    	this.email = email;
	    	this.salary = salary;
	    	this.designation = designation
	    }
		
		
		
	}











}

